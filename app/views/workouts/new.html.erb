<%- model_class = Workout -%>
<div class="page-header">
  <% if user_signed_in? %>
  <center><h1>Submit Your Workout</h1></center>
</div>
<%= render :partial => 'form' %>
<% else %>
<center><h3><%= link_to "Sign Up", new_user_registration_path %> or <%= link_to "Login", new_user_session_path %> to submit your workout</h1></center>
<% end %>